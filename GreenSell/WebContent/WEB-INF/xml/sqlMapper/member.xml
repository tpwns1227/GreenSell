<insert id="insert" parameterType="com.greensell.member.beans.MemberVO">
insert into member 
values(#{email},#{password},#{name},#{nickname},#{phone},#{address},
#{bank},#{account},#{question},#{answer},#{point},#{grade},#{report},SYSDATE)
</insert>

