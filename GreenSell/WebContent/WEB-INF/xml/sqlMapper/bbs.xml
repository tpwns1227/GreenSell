<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="bbs">
  
  <!-- 게시글 리스트 받아오기 -->
  <select id="bbsselect" parameterType="String" resultType="com.greensell.beans.BbsVo">
   select * from bbs
  </select>
  
  <!-- 게시글 쓰기 -->
  <insert id="bbsinsert" parameterType="com.greensell.beans.BbsVo">
   insert into bbs (title,bbscontent,email,bbsdate,hits,bbsno) values()
  </insert>
  
  <!-- 게시글 수정 -->
  <update id="bbsupdate" parameterType="String">
  
  </update>
  
  <!-- 게시글 삭제 -->
  <delete id="bbsdelete" parameterType="int">
  <!-- no로 삭제 -->
  
  </delete>
  
</mapper>
